<%= render "header"%>
<%= render "member_header"%>

<br/>
<h2 class="text-center">Network Contacts</h2>


<div id="member-networks-table">
  <table style="width: 90%; margin-left: auto; margin-right: auto;">

    <tr>
      <th>Photo</th>
      <th>Name</th>
      <th>Title</th>
      <th>Organization</th>
      <th>Industry</th>
      <th>View </th>
    </tr>
    <% @contacts.each do |contact| %>
    <tr>
      <td>pic</td>
      <td><%=contact.first_name + ' ' + contact.last_name%></td>
      <td><%= contact.title %></td>
      <td><%= contact.organization %></td>
      <td><%= link_to("RSVP Form", rsvp_form_event_path(event)) %></td>
      <td><%= event.feedback_link %></td>
      <td><%= link_to("View", event_path(event)) %></td>
      <td><%= link_to("Edit", edit_event_path(event)) %></td>
      <td>CSV</td>
      <td><%= link_to("Delete", delete_event_path(event)) %> </td>
    </tr>
    <% end %>
  </table>
</div>
<br/>
<br/>